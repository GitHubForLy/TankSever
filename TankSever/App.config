    <typeAliases>
      <!--别名格式: <typeAlias alias="别名" type="完整限定名,程序集名" />-->
      <typeAlias alias="IProtocolHandler" type="ServerCommon.Protocol.IProtocolHandler,ServerCommon" />
      <typeAlias alias="ProtobufHandler" type="ProtobufProto.ProtobufHandler,ProtobufProto" />
      <typeAlias alias="IActionExecuter" type="ServerCommon.Protocol.IActionExecuter,ServerCommon" />
      <typeAlias alias="ProtobufActionExecuter" type="ProtobufProto.ProtobufActionExecuter,ProtobufProto" />
      <typeAlias alias="AsyncSocketServerBase" type="ServerCommon.NetServer.AsyncSocketServerBase,ServerCommon" />
      <typeAlias alias="StandTcpServer" type="NetCore.Server.StandTcpServer,NetCore" />
      <typeAlias alias="IDBExecuterFactory" type="ServerCommon.DB.IDBExecuterFactory,ServerCommon" />
      <typeAlias alias="MySqlDBExecuterFactory" type="DBCore.MySql.MySqlDBExecuterFactory,DBCore" />
    </typeAliases>
   
    <containers>
      <container name="Server">
        <!-- 采用protobuf协议  -->
        <register type="IProtocolHandler" mapTo="ProtobufHandler" />
        <register type="IActionExecuter" mapTo="ProtobufActionExecuter" />
        <register type="AsyncSocketServerBase" mapTo="StandTcpServer" name="MainTcpServer" />
        <!-- 采用Mysql数据库  -->
        <register type="IDBExecuterFactory" mapTo="MySqlDBExecuterFactory" >
          <constructor>
            <param name="connectString" type="System.String" value="datasource=47.117.130.65;database=tank;user=root;password=05168091;
                   Pooling=true; Min Pool Size=1; Max Pool Size=10;ConnectionTimeout=3" />
          </constructor>
        </register>
      </container>
